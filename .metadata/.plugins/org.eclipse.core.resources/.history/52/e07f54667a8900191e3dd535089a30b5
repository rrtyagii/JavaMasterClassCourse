package abilities;

public class Main {
  public static void main(String[] args) {
	
}
   private static int divideLBYL(int x, int y) {
	   if(y!=0) {
		   return x/y; 
	   }
	   return 0 ; 
   } 
   
   private static int divideEAPL(int x, int y) {
	   try {
		   return x/y; 
	   }catch(ArithmeticException e) {
		   return 0; 
	   }
	   
   }
}
