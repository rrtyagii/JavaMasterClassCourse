package com;
import java.util.*; 
public class Contacts {
   private static Scanner scan=new Scanner (System.in); 
    private String name; 
    private int number; 
	 
   public Contacts(String name, int number) {
	   this.name=name; 
	   this.number=number; 
   }

public String getName() {
	return name;
}

public int getNumber() {
	return number;
}

public static Contacts createContact(String name, String number) {
	return Contacts(name, number); 
}

   
}
